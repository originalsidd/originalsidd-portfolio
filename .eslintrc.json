{
    "root": true,
    "extends": "next",
    "rules": {
        "no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ],
        "react/no-unescaped-entities": "off",
        "@next/next/no-page-custom-font": "off",
        "react/jsx-key": "off",
        "react-hooks/exhaustive-deps": "off",
        "react/jsx-no-target-blank": "off",
        "react/jsx-no-bind": "off",
        "react/display-name": "off"
    }
}
